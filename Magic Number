#include <stdio.h>

int sumDigits(int n) {
    int sum = 0;
    while (n > 0) {
        sum += n % 10;
        n /= 10;
    }
    return sum;
}

int main() {
    int n;

    printf("Enter a number: ");
    scanf("%d", &n);

    while (n > 9) {
        n = sumDigits(n);
    }

    if (n == 1)
        printf("Magic Number");
    else
        printf("Not a Magic Number");

    return 0;
}
